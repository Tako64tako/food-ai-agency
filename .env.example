# OpenAI API設定
OPENAI_API_KEY=your_openai_api_key_here

# Google Places API設定 (レストラン検索用)
GOOGLE_PLACES_API_KEY=your_google_places_api_key_here

# 食べログ予約設定
# 食べログのアカウント情報（オプション）
# 自動ログインを有効にする場合は以下を設定
TABELOG_EMAIL=your_email@example.com
TABELOG_PASSWORD=your_password

# デバッグ設定
DEBUG_MODE=true
HEADLESS_BROWSER=false  # false にするとブラウザが表示されます